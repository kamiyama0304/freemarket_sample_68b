.wrapper
  .containerCategory
    %h2.containerCategory__title
      ピックアップカテゴリー
    .containerCategory__product
      .containerCategory__product--head
        = link_to "#", class: "products--head" do
          %h3.head 新規投稿商品
      .containerCategory__product--lists
      
        .containerCategory__product--lists__list
        - @products.each do |product|
          = link_to "#", class: "listBtn" do
            %figure.product__images
              = product.images.name.inspect
              -# = image_tag product.images[0].url , alt: "商品画像", class: "product__images--image"
            .containerCategory__product--lists__list--body
              %h3.product--name
                = product.name
              .containerCategory__product--lists__list--body__details
                %ul.details__menu
                  %li
                    = product.price
                    円
                  %li ★
              %p.taxIncluded （税込）

